1 bool armonie(int x,int y)
{
    int i;
    int sx;
    int sy;
    for(i=1;i*i<x;i++){
        if(x%i==0)
        {
            sx+=i;
            sx+=x/i;
        }
    }
    if(i*i==x)sx+=i;
    for(int i=1;i*i<y;i++){
        if(y%i==0)
        {
            sy+=i;
            sy+=y/i;
        }
    }
    if(i*i==y)sy+=i;
    if(sx>sy)
    {
        int aux=sx;
        sx=sy;
        sy=aux;
    }
    if(sx<x+y&&x+y<sy)return 1;
    else return 0;
}
